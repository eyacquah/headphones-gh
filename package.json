{
  "name": "headphones-gh",
  "version": "1.0.0",
  "description": "Simple e-commerce application using the cartzilla theme",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "SET NODE_ENV=production&& nodemon server.js",
    "debug": "ndb server.js",
    "watch": "parcel watch --no-hmr ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
