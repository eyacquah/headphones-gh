    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.2",
    "express-paginate": "^1.0.2",
    "express-rate-limit": "^5.2.3",
    "helmet": "^4.4.1",
    "hpp": "^0.2.3",
    "mongoose": "^5.11.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "parcel": "1.12.3",
    "paystack": "^2.0.1",
    "pug": "^3.0.0",
    "regenerator-runtime": "^0.13.7",
    "sharp": "^0.27.1",
